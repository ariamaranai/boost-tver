{
  "manifest_version": 3,
  "name": "boost-tver",
  "version": "0",
  "description": "Boost and search for tver.jp",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "tv"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "content_scripts": [{
    "css": ["main.css"],
    "js": ["main.js"],
    "matches": ["https://tver.jp/*"],
    "world": "MAIN",
    "run_at": "document_start"
  }]
}